{
  "name": "Groovey Meta Getta",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Grooveshark Now Playing Metadata Json exporter",
  "content_scripts": [
    {
      "matches": ["http://grooveshark.com//*","https://grooveshark.com/*"],
      "js": ["scripts/jquery-1.7.1.min.js", "scripts/extension.js"],
      "run_at": "document_end"
    }
  ]
}
